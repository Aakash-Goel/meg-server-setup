{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "prettier"
  ],
  "plugins": [
    "prettier",
    "import"
  ],
  "rules": {
    "no-unused-vars": "error",
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, {"commonjs": true, "amd": true}],
    "prettier/prettier": 2
  }
}
